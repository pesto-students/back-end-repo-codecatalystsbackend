    "/v1/api/interview/{interview_id}/submit": {
      "post": {
        "summary": "Submit Interview",
        "tags": [
          "Submit Interview"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "interview_id",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "ID of the interview"
          },
          {
            "in": "query",
            "name": "token",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Authentication token"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "question_id": {
                      "type": "string"
                    },
                    "answer": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
